      "get": {
        "tags": [
          "Players"
        ],
        "parameters": [
          {
            "name": "username",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/PlayerInfoDto"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PlayerInfoDto"
